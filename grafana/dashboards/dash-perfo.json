          "metricColumn": "player_name",
          "rawQuery": true,
          "rawSql": "SELECT \n  ROW_NUMBER() OVER (ORDER BY fp.performance_rating DESC) as \"Rank\",\n  p.player_name as \"Player Name\",\n  c.country_name as \"Country\",\n  p.age as \"Age\",\n  pos.position_name as \"Position\",\n  fp.performance_rating as \"Performance Rating\",\n  fp.goals as \"Goals\",\n  fp.assists as \"Assists\",\n  fp.goal_contributions as \"Total Contributions\",\n  fp.pass_accuracy_pct as \"Pass %\",\n  fp.minutes_played as \"Minutes\",\n  fp.distance_run_km as \"Distance (km)\",\n  fp.tackles as \"Tackles\",\n  fp.interceptions as \"Interceptions\",\n  fp.yellow_cards as \"Yellows\",\n  fp.red_cards as \"Reds\",\n  fp.created_at as \"Updated At\"\nFROM fact_performance fp\nJOIN dim_player p ON fp.player_id = p.player_id\nJOIN dim_country c ON fp.country_id = c.country_id\nJOIN dim_position pos ON fp.position_id = pos.position_id\nWHERE (pos.position_name = '${position:raw}' OR '${position:raw}' = 'All')\n  AND p.age <= ${max_age:raw}\n  AND fp.performance_rating >= ${min_rating:raw}\n  AND fp.minutes_played > 0\nORDER BY fp.performance_rating DESC\nLIMIT 15",
          "refId": "A"
        }
